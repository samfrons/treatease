body {
  font-family: "Open Sans", sans-serif;
}
* {
  font-family: "Open Sans", sans-serif ;
  -webkit-font-smoothing: antialiased;
}

.owl-theme .owl-controls .owl-page span {
display: block;
width: 12px;
height: 12px;
margin: 5px 7px;
filter: Alpha(Opacity=50);
opacity: 0.5;
-webkit-border-radius: 20px;
-moz-border-radius: 20px;
border-radius: 20px;
background: #869791;
}

section.quiz {
  padding: 0;
  background: white;
  box-shadow: 0 1px 10px 1px rgba(58,58,58,.3)
}

.owl-controls.clickable {
  position: relative;
top: 70px;
}

.owl-theme .owl-controls .owl-buttons div {
color: #FFF;
display: inline-block;
zoom: 1;
margin: 5px;
padding: 3px 10px;
font-size: 12px;
-webkit-border-radius: 30px;
-moz-border-radius: 30px;
border-radius: 30px;
background: #869791;
filter: Alpha(Opacity=50);
opacity: 0.5;
width: 100px;
height: auto;
position: static;
}

.owl-pagination {
    position: static;
}

.aqua.main-question {
    background: rgb(05,219,194) !important;
    color: white;
    padding: 15px;
    font-size: 1.5em;
    font-weight: 600;
    margin: 0;
    opacity: 1;

}


h3.risk {
  color: white;
    padding: 15px;
    font-size: 1.8em;
    font-weight: 500;
    background: #f23e47;
    max-width: 700px;
    margin: 20px auto;
    text-align: center;
    border-radius: 5px;
}

h3.risk > span {
  margin-left: 10px;
  -webkit-font-smoothing: antialiased;
}

.sub-question {
  padding: 10px;
  margin: 0 auto;
max-width: 700px;

}

.sub-question h3 {
  font-size: 1.3em;
    font-weight: 500;
}

ul.responses {
  width: 100%;
  margin-bottom: 0;
/*border: 1px solid #ccc; */
border-radius: 3px;
padding: 0;
}

ul.responses.q1 {
  max-width: 500px;
}
ul.responses li {
  display: inline-block;
/*border-right: 1px solid #ccc; */
padding: 10px;
text-align: center;
background: #fafafa;
border-radius: 3px;
font-weight: 500;
-webkit-font-smoothing: initial;
}

ul.responses li.last {
  border-right: none;
}

ul.responses li.selected {
  
  background: #1ebebc;
  color: white;
}


.substance h4{
  font-weight: 500;
}

.middle-max {
  max-width: 700px;
  margin: 0 auto;
  padding: 10px;
}


  .dmbutton {
    border: 3px solid rgb(210, 221, 40) !important;
    background: transparent;
    color:rgb(210, 221, 40);
    -webkit-border-radius: 2px;
    border-radius: 2px;
    padding-top: 1.025rem;
    padding-right: 2.25rem;
