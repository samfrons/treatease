  h3.rec.high {
    color: #f23e47;
  }

  h3.rec.moderate {
    color: rgb(210, 221, 40);
  }

  h3.rec.low {
    color: rgb(05,219,194);
  }

  .plan {
    text-align: center;
  }

  .owl-prev {
    float: left;
  }

  .owl-next {
    float: right;
  }

