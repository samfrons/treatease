  .level {
    color: white;
    border-radius: 5px;
    padding: 5px;
    font-size: .7em;
    font-weight: 500;
    margin: 10px;
    -webkit-font-smoothing: initial;
  }

  .ratio {
    margin-left: 10px;
    font-size: .8em;
    font-weight: bolder;
    -webkit-font-smoothing: initial;
  }

  .level.high {
    background: #f23e47 !important;
  }

  .level.moderate {
    background: rgb(210, 221, 40) !important;
  }

  .level.low {
    
    background: rgb(05,219,194);
  }

  h3.rec {
    font-size: 1em;
  }

  h4.rec.test {
    color: #f23e47;
  }
  h4.rec > span {
    margin-left: 10px;
